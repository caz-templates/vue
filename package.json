{
  "name": "@caz-templates/vue",
  "version": "0.1.0",
  "private": true,
  "description": "A template for creating vue.js apps.",
  "keywords": [
    "vue",
    "caz-templates",
    "caz",
    "zce"
  ],
  "repository": "caz-templates/vue",
  "license": "MIT",
  "author": "zce <w@zce.me> (https://zce.me)",
  "main": "index.js",
  "scripts": {
    "test": "standard --fix && jest"
  },
  "jest": {
    "testTimeout": 30000,
    "modulePathIgnorePatterns": [
      "<rootDir>/template"
    ]
  },
  "renovate": {
    "extends": [
      "zce"
    ],
    "ignorePaths": [
      "template"
    ]
  },
  "standard": {
    "env": [
      "jest"
    ],
    "ignore": [
      "template"
    ]
  },
  "dependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@vue/cli-plugin-babel": "4.5.11",
    "@vue/cli-plugin-eslint": "4.5.11",
    "@vue/cli-service": "4.5.11",
    "@vue/eslint-config-standard": "6.0.0",
    "axios": "0.21.1",
    "core-js": "3.8.3",
    "element-ui": "2.15.0",
    "nprogress": "0.2.0",
    "store": "2.0.12",
    "vue": "2.6.12",
    "vue-i18n": "8.22.4",
    "vue-router": "3.5.1",
    "vuex": "3.6.2",
    "babel-eslint": "10.1.0",
    "eslint": "7.18.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.3.1",
    "eslint-plugin-standard": "5.0.0",
    "eslint-plugin-vue": "7.5.0",
    "husky": "4.3.8",
    "lint-staged": "10.5.3",
    "sass": "1.32.5",
    "sass-loader": "10.1.1",
    "vue-template-compiler": "2.6.12"
  },
  "devDependencies": {
    "caz": "0.2.1",
    "jest": "26.6.3",
    "standard": "16.0.3"
  }
}
